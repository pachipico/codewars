public class 2023.05.05 {

    public void unluckyDays() {
        int cnt = 0;
        Calendar calendar = Calendar.getInstance();

        for (int i = 0; i < 12; i++) {
            calendar.set(2015, Calendar.JANUARY, 12);
            if(calendar.get(Calendar.DAY_OF_WEEK) == 5) cnt++;
        }

    }
}
