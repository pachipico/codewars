// When provided with a number between 0-9, return it in words.

// Input :: 1

// Output :: "One".

// If your language supports it, try using a switch statement.

// 내 답안
import java.util.Map;

public class Kata {

  public static String switchItUp(int number) {
    return Map
      .of(
        0,
        "Zero",
        1,
        "One",
        2,
        "Two",
        3,
        "Three",
        4,
        "Four",
        5,
        "Five",
        6,
        "Six",
        7,
        "Seven",
        8,
        "Eight",
        9,
        "Nine"
      )
      .get(number);
  }
}

// 모범답안
public class Kata {

  public static String switchItUp(int number) {
    return new String[] {
      "Zero",
      "One",
      "Two",
      "Three",
      "Four",
      "Five",
      "Six",
      "Seven",
      "Eight",
      "Nine",
    }[number];
  }
}
